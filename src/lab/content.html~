                                dvsPlayer_2.settings=sett;		
                                dvsPlayer_2.Init('dvsPlayer_2');
                                dvsPlayer_2.dvs_play_control.style.backgroundColor='#3D60A5';
                        </script>
                        <script type="text/javascript">
                                var dvsPlayer_3 = new DVS_html5player();
                                var skin = new LineSimpleSkinPlay();
                                var sett = new dvsplay_settings();
                                sett.autoSize=true;
                                dvsPlayer_3.Skin = skin;
                                dvsPlayer_3.settings=sett;		
                                dvsPlayer_3.Init('dvsPlayer_3');
                                dvsPlayer_3.dvs_play_control.style.backgroundColor='#3D60A5';
                        </script>
                        <script type="text/javascript">
